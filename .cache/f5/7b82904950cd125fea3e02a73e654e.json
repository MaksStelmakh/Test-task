or: #ffffff;\r\n  text-align: center;\r\n  height: 40px;\r\n\r\n  @media screen and (min-width: 768px) {\r\n    display: none;\r\n  }\r\n}\r\n\r\n#check-menu {\r\n  display: none;\r\n}\r\n\r\n.label {\r\n  display: block;\r\n  position: relative;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  cursor: pointer;\r\n  z-index: 2;\r\n  line-height: 40px;\r\n}\r\n\r\n.burger-line {\r\n  position: absolute;\r\n  top: 8px;\r\n  right: 50%;\r\n  width: 35px;\r\n  height: 2px;\r\n  background-color: var(--black);\r\n  transition: 0.3s all;\r\n}\r\n\r\n.burger-line:nth-of-type(2),\r\n.burger-line:nth-of-type(3) {\r\n  top: 18px;\r\n}\r\n\r\n.burger-line:nth-of-type(4) {\r\n  top: 28px;\r\n}\r\n\r\n.main-menu {\r\n  display: block;\r\n  position: absolute;\r\n  width: 100%;\r\n  bottom: 0;\r\n  left: -17px;\r\n  transform: translateY(100%);\r\n  max-height: 0;\r\n  font-size: 0;\r\n  overflow: hidden;\r\n  transition: 0.3s all;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.main-menu a {\r\n  display: block;\r\n  width: 100%;\r\n  padding: 10px;\r\n  color: var(--black);\r\n  background-color: none;\r\n  text-decoration: none;\r\n  border-top: 1px solid gray;\r\n}\r\n\r\n.main-menu a:hover {\r\n  background-color: orange;\r\n}\r\n\r\n#check-menu:checked ~ .main-menu {\r\n  max-height: 500px;\r\n  font-size: 16px;\r\n}\r\n\r\n#check-menu:checked ~ .burger-line:nth-of-type(1) {\r\n  display: none;\r\n}\r\n\r\n#check-menu:checked ~ .burger-line:nth-of-type(2) {\r\n  transform: rotate(45deg);\r\n}\r\n\r\n#check-menu:checked ~ .burger-line:nth-of-type(3) {\r\n  transform: rotate(-45deg);\r\n}\r\n\r\n#check-menu:checked ~ .burger-line:nth-of-type(4) {\r\n  display: none;\r\n}\r\n\r\n// Выбор языка\r\n\r\n.custom-select {\r\n  margin-left: 32px;\r\n  margin-top: 1px;\r\n  position: relative;\r\n  z-index: 11;\r\n  @media screen and (min-width: 768px) {\r\n    margin-right: 30px;\r\n  }\r\n\r\n  @media screen and (min-width: 1444px) {\r\n    margin-right: 89px;\r\n  }\r\n\r\n  & select {\r\n    display: none;\r\n  }\r\n}\r\n\r\n.menu-open {\r\n  position: absolute;\r\n  margin-left: 35px;\r\n  margin-top: 15px;\r\n}\r\n\r\n.select-items {\r\n  width: 244px;\r\n  height: 138px;\r\n  position: absolute;\r\n  top: 170%;\r\n  right: -9px;\r\n  z-index: 99;\r\n  background-color: #ffffff;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);\r\n\r\n  & div {\r\n    padding: 5px 16px;\r\n  }\r\n\r\n  & div:hover {\r\n    background-color: var(--gray-light);\r\n  }\r\n\r\n  @media screen and (max-width: 767px) {\r\n    width: 200px;\r\n  }\r\n}\r\n\r\n.select-items div,\r\n.select-selected {\r\n  position: relative;\r\n  cursor: pointer;\r\n}\r\n\r\n.select-items div,\r\n.select-selected,\r\n.icon1 {\r\n  position: relative;\r\n  cursor: pointer;\r\n  display: block;\r\n}\r\n\r\n.select-hide {\r\n  display: none;\r\n}\r\n\r\n//   @media screen and (min-width: 320px) and (max-width: 767px) {\r\n//     max-width: 480px;\r\n//   }\r\n//   @media screen and (min-width: 768px) {\r\n//     max-width: 688px;\r\n//   }\r\n//   @media screen and (min-width: 1444px) {\r\n//     max-width: 1100px;\r\n//   }\r\n//   @media screen and (min-width: 1600px) {\r\n//     max-width: 1400px;\r\n//   }\r\n// }\r\n"},"lineCount":null}},"error":null,"hash":"1b987cdae3f9f7f025169659e8852019","cacheData":{"env":{}}}